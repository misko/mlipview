{
  "name": "mlipviewer2",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:fast": "MLIPVIEW_SKIP_SERVERS=1 jest --detectOpenHandles=false --forceExit=false",
    "test:integration": "jest -c jest.integration.config.js",
    "gen:proto:js": "protoc --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es --es_out=public/proto --es_opt=target=js fairchem_local_server2/session.proto",
    "predev": "npm run gen:proto:js",
    "prebuild": "npm run gen:proto:js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --strictPort --port 5174"
  },
  "dependencies": {
    "babylonjs": "^7.25.0",
    "express": "^4.19.2",
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@bufbuild/protobuf": "^2.9.0",
    "@bufbuild/protoc-gen-es": "^2.9.0",
    "@playwright/test": "^1.55.1",
    "babel-jest": "^29.7.0",
    "canvas": "^2.11.2",
    "cross-fetch": "^4.0.0",
    "google-protobuf": "4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "protoc-gen-js": "^3.21.4-4",
    "supertest": "^6.3.4",
    "vite": "^7.1.7"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
