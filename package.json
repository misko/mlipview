{
  "name": "mlipviewer2",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:fast": "MLIPVIEW_SKIP_SERVERS=1 jest --detectOpenHandles=false --forceExit",
    "test:integration": "jest -c jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test tests-e2e/smoke-*",
    "test:shared": "node scripts/test-by-spec.cjs",
    "test:py": "./mlipview_venv/bin/python -m pytest -q",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "gen:proto:js": "protoc --plugin=protoc-gen-es=./node_modules/.bin/protoc-gen-es --es_out=public/proto --es_opt=target=js fairchem_local_server2/session.proto && mkdir -p legacy_proto/fairchem_local_server2 && cp public/proto/fairchem_local_server2/session_pb.js legacy_proto/fairchem_local_server2/session_pb.js",
    "predev": "npm run gen:proto:js",
    "prebuild": "npm run gen:proto:js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --strictPort --port 5174"
  },
  "dependencies": {
    "@babylonjs/core": "^8.33.4",
    "@babylonjs/gui": "^8.33.4",
    "@babylonjs/loaders": "^8.33.4",
    "express": "^4.19.2",
    "jsdom": "^22.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.26.0",
    "@bufbuild/protobuf": "^2.9.0",
    "@bufbuild/protoc-gen-es": "^2.9.0",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "canvas": "^2.11.2",
    "cross-fetch": "^4.0.0",
    "google-protobuf": "4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "protoc-gen-js": "^3.21.4-4",
    "supertest": "^6.3.4",
    "typescript": "^5.6.3",
    "vite": "^7.1.7"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
